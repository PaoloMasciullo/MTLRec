{
  "model": "HUIM",
  "config": {
    "embedding_dim": 128,
    "attention_hidden_sizes": [128],
    "attention_hidden_activations": "Dice",
    "dnn_hidden_sizes": [256, 128, 64],
    "dnn_hidden_activations": "ReLu",
    "dnn_output_size": 1,
    "dnn_output_activation": "Sigmoid",
    "dnn_dropout": 0.5,
    "gate_hidden_sizes": [128],
    "gate_hidden_activations":"ReLu",
    "use_batchnorm": true,
    "target_features": [ "article_id", "category", "topic1", "sentiment_label"],
    "sequence_features": [ "hist_id", "hist_cat", "hist_topic1", "hist_sentiment", "hist_read_time", "hist_scroll_percent"],
    "target_pretrained_multimodal_embeddings": [ "article_id_img", "article_id_txt" ],
    "sequence_pretrained_multimodal_embeddings": [ "hist_id_img", "hist_id_txt" ],
    "attention_dropout": 0.5,
    "use_softmax": false
  }
}
